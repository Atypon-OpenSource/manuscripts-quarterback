{
  "name": "@manuscripts/quarterback-schema",
  "version": "0.0.1",
  "author": "Atypon Systems LLC",
  "license": "Apache-2.0",
  "homepage": "https://gitlab.com/mpapp-public/manuscripts-quarterback",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.es.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "format": "prettier --write \"*.+(js|json|yml|yaml|ts|md|graphql|mdx)\" src/",
    "lint": "eslint --cache --ext .ts ./src ./types",
    "lint:fix": "eslint --fix --ext .ts, ./src ./types",
    "watch": "rollup -cw"
  },
  "devDependencies": {
    "@manuscripts/quarterback-editor": "*",
    "@manuscripts/quarterback-shared": "*",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@types/prosemirror-model": "^1.13.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.32.0",
    "prettier": "^2.4.1",
    "prosemirror-model": "^1.14.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "prosemirror-model": ">=1.14.0"
  }
}
