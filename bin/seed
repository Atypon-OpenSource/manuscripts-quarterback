#!/usr/bin/env ts-node

/* eslint-disable */

const { Sequelize } = require('sequelize')
const config = require('../config')

const { dbCxn } = config

const db = new Sequelize(dbCxn)

;(async () => {
  try {
    await db.authenticate()

    // create tables here

    await db.close()
  } catch (e) {
    console.error('No datatbase connection')
    process.exit(1)
  }

  process.exit()
})()
